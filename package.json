{
  "name": "extrafm-zagreb-player",
  "productName": "ExtraFM Zagreb",
  "version": "1.3.1",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --watch --env=dev",
    "build": "yarn clean && webpack --env=prod",
    "clean": "rm -rf dist/",
    "startapp": "yarn build && electron . --env=dev",
    "packapp": "yarn build && electron-packager . --overwrite --icon=./app/assets/images/icon.icns",
    "packappall": "yarn packapp --platform=darwin,linux,win32"
  },
  "dependencies": {
    "axios": "0.19.2",
    "howler": "2.2.0",
    "normalize.css": "8.0.1",
    "typed.js": "2.0.11",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "babel-loader": "8.1.0",
    "css-loader": "3.5.3",
    "electron": "18.3.7",
    "electron-packager": "14.2.1",
    "file-loader": "6.0.0",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "path": "0.12.7",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url": "^0.11.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  }
}
