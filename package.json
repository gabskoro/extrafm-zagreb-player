{
  "name": "extrafm-zagreb-player",
  "productName": "ExtraFM Zagreb",
  "version": "1.2.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --watch --env=dev",
    "build": "yarn clean && webpack --env=prod",
    "clean": "rm -rf dist/",
    "startapp": "yarn build && electron .",
    "packapp": "yarn build && ./node_modules/electron-packager/cli.js . --overwrite --icon=./app/assets/images/icon.icns",
    "packappall": "yarn packapp --platform=darwin,linux,win32"
  },
  "dependencies": {
    "axios": "0.18.0",
    "howler": "2.1.0",
    "normalize.css": "8.0.1",
    "typed.js": "2.0.9",
    "vue": "2.5.21"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "babel-loader": "8.0.4",
    "css-loader": "2.0.1",
    "electron": "3.0.12",
    "electron-packager": "13.0.1",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "path": "0.12.7",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url": "^0.11.0",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.5"
  }
}
